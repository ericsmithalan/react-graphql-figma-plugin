{
  "compilerOptions": {
    "outDir": "lib",
    "target": "es5",
    "lib": ["dom","es6", "esnext"],
    "module": "commonjs",
    "declaration": true,
    "sourceMap": true,
    "strict": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "jsx": "react",
    "esModuleInterop": true
  },
  "include": [
    "src"
  ],
  "exclude": [
    "node_modules",
    "lib",
    "dist",
    "example",
    "**/__generated",
    ".idea"
  ]
}